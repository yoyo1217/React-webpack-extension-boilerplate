{
  "manifest_version": 3,
  "name": "React Extension",
  "description": "React Extension Boilerplate",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html",
    "default_title": "React Extension Boilerplate"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}